{
  "name": "bedownloader",
  "version": "1.0.1",
  "description": "",
  "main": "main.js",
  "repo": "https://github.com/ViRT1ST/BeDownloader.git",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/img/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=app/img/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron App\"",
    "package-lnx": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=app/img/icons/Icon_1024x1024.png --prune=false --out=release-builds"
  },
  "author": "ViRT1ST",
  "license": "ISC",
  "dependencies": {
    "electron": "^23.2.1",
    "ini": "^4.0.0",
    "node-fetch": "^2.6.9",
    "piexifjs": "^1.0.6",
    "puppeteer": "^19.7.5",
    "slash": "^3.0.0",
    "transliteration": "^2.3.5"
  },
  "devDependencies": {
    "electron-packager": "^17.1.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7"
  }
}
